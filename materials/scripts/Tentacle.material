import base_material from "general.material"
material Tentacle : base_material
{
    receive_shadows on
    technique
    {
        pass
        {
            texture_unit
            {
                texture Tentakel_diff_yellow7.png
            }
        }
    }

    set_texture_alias DiffuseMap Tentakel_diff_yellow7.png
    set_texture_alias SpecMap Tentakel_diff_yellow7.png
    set_texture_alias NormalMap Tentakel_diff_nor.png
}
