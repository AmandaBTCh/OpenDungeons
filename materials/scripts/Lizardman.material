import base_material from "general.material"
material Lizardman : base_material
{
    technique
    {
        pass 0
        {
            texture_unit
            {
                texture Lizardman_col.png
            }
        }
    }

    set_texture_alias diffuseMap Lizardman_col.png
    set_texture_alias specMap Lizardman_spec.png
    set_texture_alias normalMap Lizardman_nor3.png
}
