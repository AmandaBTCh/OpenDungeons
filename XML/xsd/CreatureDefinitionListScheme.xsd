<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2005/08/xml.xsd"/>     
    <xs:include schemaLocation="CreatureDefinitionScheme.xsd"/>
    <xs:group name="CreatureDefinitionList" >
        <xs:sequence>
            <xs:element name="creatureDefinitionList" >
                <xs:complexType >
                    <xs:sequence maxOccurs="unbounded">
                        <xs:group ref="CreatureDefinition"/>
                    </xs:sequence>
                    <xs:attribute ref="xml:base"/>              
                </xs:complexType >
            </xs:element>
        </xs:sequence>        
    </xs:group>  
    
    
    <xs:element name="creatureDefinitionList" >
        <xs:complexType >
            <xs:sequence maxOccurs="unbounded">
                <xs:group ref="CreatureDefinition"/>
            </xs:sequence>
        </xs:complexType >
    </xs:element>
</xs:schema>